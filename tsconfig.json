{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs",
    "lib": [
      "esnext.asynciterable","esnext", "dom"
    ],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./lib",
    "rootDir": "./src",
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": ".",

    "esModuleInterop": true
  },
  "exclude": [
    "node_modules/**",
    "node_modules/.pnpm",
    "lib"
  ]
}